<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.MatriculaPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker01" Title="Aluno"></Picker>
            <Picker x:Name="Picker02" Title="Disciplina"></Picker>
            <Entry x:Name="Entry01" Keyboard="Numeric" Placeholder="Periodo" />
            <Button x:Name="Button" Text="Matriclular"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked" />

            <Label x:Name="label01" Text=""/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>